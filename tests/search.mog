program test;

var int a, b, LEN, result;
var int A[10];

instr int search(int to_find){
    a = 0;
    while (a<LEN){
        if (to_find == A[a]){
            return a;
        }
        a = a + 1;
    }
    return 9999999;
}
main{
    LEN = 10;
    a = 0;
    while (a<LEN){
        A[a] = a*a;
        a = a + 1;
    }
    a = 0;
    while (a<LEN){
        output-> A[a];
        a = a + 1;
    }

    input <- b;
    result = _search(b);
    output -> "The result is in box #";
    output -> result;

    return 0;
}